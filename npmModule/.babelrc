//babelとは、ES6のコードをES5に変換するツール 
{
  "presets": [
    "@babel/preset-react",
    ["@babel/preset-env", {
      "targets": {
          "ie": 11,
          "safari": 9
      },
      "useBuiltIns": "usage",
      "corejs": 3
    }]
  ],
  "env": {
    "development": {
      "plugins": [
        [
          "babel-plugin-styled-components", {
            "minify": false
          },
          "react-hot-loader/babel"
        ]
      ]
    }
  }
}
